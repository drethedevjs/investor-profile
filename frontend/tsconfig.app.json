{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "frontend/src/**/*.ts",
    "frontend/src/**/*.d.ts",
    "frontend/src/**/*.vue"
  ],
  "exclude": ["frontend/src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "frontend/node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "paths": {
      "@/*": ["frontend/src/*"]
    }
  }
}
